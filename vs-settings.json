{
    "editor.renderWhitespace": "all",
    "editor.wordWrap": "on",
    "editor.fontFamily": "Operator Mono Book, Cartograph CF, Ellograph CF, Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 2,
    "editor.minimap.maxColumn": 90,
    "editor.minimap.showSlider": "always",
    "workbench.iconTheme": "material-icon-theme",
    "editor.codeActionsOnSave": {

    },
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "files.trimTrailingWhitespace": true,
    "editor.minimap.renderCharacters": false,
    "editor.scrollbar.horizontalScrollbarSize": 4,
    "editor.scrollbar.verticalScrollbarSize": 4,
    "editor.rulers": [
        80,
        120
    ],
    "redhat.telemetry.enabled": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.title": "${activeEditorLong}${separator}${rootName}${separator}${profileName}",
    "editor.fontSize": 11,
    "editor.fontWeight": "normal",
    "workbench.colorTheme": "sprayed",
    "terminal.integrated.fontWeight": "normal",
    "files.insertFinalNewline": true,
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "indentRainbow.colors": [
        "rgba(130,130,130,0.1)",
        "rgba(130,130,130,0.2)",
        "rgba(130,130,130,0.3)",
        "rgba(130,130,130,0.4)",
        "rgba(130,130,130,0.5)",
        "rgba(130,130,130,0.6)",
        "rgba(130,130,130,0.7)",
        "rgba(130,130,130,0.8)",
    ],
    "indentRainbow.errorColor": "rgba(255, 0, 0, 0.75)",
    "indentRainbow.tabmixColor": "rgba(255, 0, 0, 0.75)",
    "indentRainbow.updateDelay": 1000,
    "indentRainbow.ignoreLinePatterns": [
        "/[ \t]* [*]/g",
    ],
}
